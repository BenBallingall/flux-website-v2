{
  "name": "voteflux",
  "version": "1.0.0",
  "description": "Flux website",
  "main": "index.js",
  "scripts": {
    "flux": "jekyll 'clean' && parallelshell 'gulp' 'bundle exec jekyll build --watch --config _config.yml'",
    "flux-no-react": "bundle exec jekyll serve --watch --config _config.yml",
    "deploy-stage": "jekyll clean && gulp 'sass' | exit 0 && cd 'react-signup/' && webpack -p && cd '../' && jekyll build && gulp 'deploy' && exit",
    "build": "jekyll clean && gulp 'sass' | exit 0 && cd 'react-signup/' && webpack -p && cd '../' && jekyll build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "David Factor",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "browser-sync": "^2.11.2",
    "concurrently": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-clean-css": "^2.0.5",
    "gulp-gh-pages": "^0.5.4",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^2.2.0",
    "parallelshell": "^2.0.0"
  },
  "dependencies": {
    "formsy-react": "^0.17.0",
    "jquery-touch-events": "^1.0.3",
    "lodash.throttle": "^4.0.1",
    "moment": "^2.13.0",
    "react": "^0.14.8"
  }
}
